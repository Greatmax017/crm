"use strict"
var myApp = angular.module('rout', []);
myApp.config(["$stateProvider", "$urlRouterProvider", routeFn]);
function routeFn($stateProvider, $urlRouterProvider) {
    $urlRouterProvider.when("/center", "/center/AccountInfo");
    $urlRouterProvider.when("/center/manage", "/center/manage/deposit");
    $urlRouterProvider.otherwise("login");//路由跳转到登录页面
    // $urlRouterProvider.when("/en/index", "/en/index");
    $stateProvider
        .state("login", {//登录
            url: "/login",
            templateUrl: "/view/login.html?d=" + new Date().getTime(),
            // controllerUrl: '/min/login.min.js',
            controller: "loginController"
        })
        .state("membertype", {//开户
            url: "/membertype",
            templateUrl: "/view/membertype.html?d=" + new Date().getTime(),
            controller: "membertypeController",
            // controllerUrl: '/min/membertype.min.js'
        }).state("payhtml", {//开户
            url: "/payhtml",
            templateUrl: "/view/common/payHtml.html?d=" + new Date().getTime(),
            controller: "payController",
            // controllerUrl: '/min/membertype.min.js'
        })
        .state("findPassword", {//找回密码
            url: "/findPassword",
            templateUrl: "/view/FindPassword.html?d=" + new Date().getTime(),
            controller: "findpasswordController",
            // controllerUrl: '/min/findpassword.min.js'
        })
        .state("resetPassword", {//重置密码
            url: "/resetPassword",
            templateUrl: "/view/ResetPassword.html?d=" + new Date().getTime(),
            controller: "resetpasswordController",
            // controllerUrl: '/app/controllers/membercenter/resetpassword.js'
        })
        .state("demoAccount", {//重置密码
            url: "/DemoAccount",
            templateUrl: "/view/DemoAccount.html?d=" + new Date().getTime(),
            controller: "DemoAccountController"
        })
        //个人中心新版
        .state("center", {//
            url: "/center",
            templateUrl: "/view/personalCenter/center.html?d=" + new Date().getTime(),
            controller: "CenterController"
        })
        .state("center.AccountInfo", {//账户概括
            url: "/AccountInfo",
            templateUrl: "/view/personalCenter/AccountInfo.html?d=" + new Date().getTime(),
            controller: "AccountInfoController"
        })
        .state("center.manage", {//出入金管理
            url: "/manage",
            templateUrl: "/view/personalCenter/management/index.html?d=" + new Date().getTime(),
            controller: "ManagementController"
        })
        .state("center.manage.deposit", {//在线入金
            url: "/deposit",
            templateUrl: "/view/personalCenter/management/deposit.html?d=" + new Date().getTime(),
            controller: "depositController"
        })
        .state("center.manage.BankDeposit", {//银行电汇
            url: "/BankDeposit",
            templateUrl: "/view/personalCenter/management/BankDeposit.html?d=" + new Date().getTime(),
            controller: "uploadController"
        })
        .state("center.manage.ApplyWithdrawals", {//申请提款
            url: "/ApplyWithdrawals",
            templateUrl: "/view/personalCenter/management/ApplyWithdrawals.html?d=" + new Date().getTime(),
            controller: "applyController"
        })
        .state("center.manage.RefundRecords", {//出金记录
            url: "/RefundRecords",
            templateUrl: "/view/personalCenter/management/RefundRecords.html?d=" + new Date().getTime(),
            controller: "refundController"
        })
        .state("center.manage.PaymentRecords", {//入金记录
            url: "/PaymentRecords",
            templateUrl: "/view/personalCenter/management/PaymentRecords.html?d=" + new Date().getTime(),
            controller: "paymentController"
        })
        .state("center.manage.History", {//盈利报告
            url: "/History",
            templateUrl: "/view/personalCenter/management/History.html?d=" + new Date().getTime(),
            controller: "historyController"
        })
        .state("center.Kyc", {//KYC认证
            url: "/Kyc",
            templateUrl: "/view/personalCenter/Kyc.html?d=" + new Date().getTime(),
            controller: "KycController"
        })
        .state("center.Accountset", {//基本设置
            url: "/Accountset",
            templateUrl: "/view/personalCenter/AccountSettings.html?d=" + new Date().getTime(),
            controller: "AccountSetController"
        })
        .state("PersonRegister", {//个人注册----注册
            url: "/PersonRegister",
            templateUrl: "/view/PersonRegister.html?d=" + new Date().getTime(),
            controller: "accountController"
        })
        .state("CompanyRegister", {//机构注册----注册
            url: "/CompanyRegister",
            templateUrl: "/view/CompanyRegister.html?d=" + new Date().getTime(),
            controller: "accountController"
        })
        .state("center.MemberUpgrade", {
            url: "/MemberUpgrade",
            templateUrl: "/view/personalCenter/MemberUpgrade.html?d=" + new Date().getTime(),
            controller: "MemberUpgradeController"
        })
}
